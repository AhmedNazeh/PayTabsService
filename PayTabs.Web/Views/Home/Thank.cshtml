<div>
    <h4> تفاصيل العملية الخاصة بكم</h4>
    <hr />
    <dl class="dl-horizontal">
    

        <dt>
           رقم الفاتوره
        </dt>

        <dd>
            @Html.DisplayFor(model => model.pt_invoice_id)
        </dd>

        <dt>
