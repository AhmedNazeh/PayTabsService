        </dt>

        <dd>
            @Html.DisplayFor(model => model.amount)
        </dd>

        <dt>
           العمله
        </dt>

        <dd>
            @Html.DisplayFor(model => model.currency)
        </dd>

        <dt>
           رقم المرجع
        </dt>

        <dd>
            @Html.DisplayFor(model => model.reference_no)
        </dd>

        <dt>
           رقم العملية
        </dt>

        <dd>
            @Html.DisplayFor(model => model.transaction_id)
        </dd>

